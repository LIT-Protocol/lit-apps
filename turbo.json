{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "pub": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "outputs": []
    }
  },
  "globalDependencies": [
    "$NODE_ENV",
    "$SERVER_PRIVATE_KEY",
    "$PINATA_API_KEY",
    "$PINATA_SECRET_KEY"
  ]
}
